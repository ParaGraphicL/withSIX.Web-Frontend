{
    "version": "2.0",
    "atom": {
        "rewriteTsconfig": true
    },
    "compileOnSave": false,
    "compilerOptions": {
        "target": "es6",
        "module": "es6",
        "moduleResolution": "node",
        "noImplicitAny": false,
        "noLib": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "sourceMap": true,
        "allowJs": false
    },
    "awesomeTypescriptLoaderOptions": {
        "doTypeCheck": true,
        "forkChecker": true,
        "useBabel": true,
        "useCache": true,
        "babelOptions": {
            "presets": [
                "es2015-loose-native-modules"
            ]
        }
    },
    "filesGlob": [
        "../typings/custom/**/*.d.ts",
        "../typings/index.d.ts",
        "typings/**/*.d.ts",
        "**/*.ts"
    ],
    "files": [
        "../typings/custom/angular-local-storage/angular-local-storage.d.ts",
        "../typings/custom/angular-ui/angular-ui-router.d.ts",
        "../typings/custom/angular-ui/angular-ui-sortable.d.ts",
        "../typings/custom/angularjs/angular-animate.d.ts",
        "../typings/custom/angularjs/angular-cookies.d.ts",
        "../typings/custom/angularjs/angular-mocks.d.ts",
        "../typings/custom/angularjs/angular-resource.d.ts",
        "../typings/custom/angularjs/angular-route.d.ts",
        "../typings/custom/angularjs/angular-sanitize.d.ts",
        "../typings/custom/angularjs/angular.d.ts",
        "../typings/custom/aurelia-auth/aurelia-auth.d.ts",
        "../typings/custom/breeze/breeze.d.ts",
        "../typings/custom/clipboard-js.d.ts",
        "../typings/custom/core-js.d.ts",
        "../typings/custom/core-js/core-js.d.ts",
        "../typings/custom/dojo/dojo.d.ts",
        "../typings/custom/es6.d.ts",
        "../typings/custom/esri/esri.d.ts",
        "../typings/custom/jquery.colorbox/jquery.colorbox.d.ts",
        "../typings/custom/jquery.cookie/jquery.cookie.d.ts",
        "../typings/custom/jquery.timeago/jquery.timeago.d.ts",
        "../typings/custom/jquery/jquery.d.ts",
        "../typings/custom/linq/linq.d.ts",
        "../typings/custom/modernizr/modernizr.d.ts",
        "../typings/custom/moment/moment-node.d.ts",
        "../typings/custom/moment/moment.d.ts",
        "../typings/custom/nprogress.d.ts",
        "../typings/custom/numbro.d.ts",
        "../typings/custom/phantomjs/phantomjs.d.ts",
        "../typings/custom/reflect-metadata.d.ts",
        "../typings/custom/signalr/signalr.d.ts",
        "../typings/custom/typeahead/typeahead.d.ts",
        "../typings/custom/version_compare.d.ts",
        "../typings/index.d.ts",
        "typings/chai.d.ts",
        "typings/mocha.d.ts",
        "unit/spec/helpers/bbcode.ts",
        "unit/spec/resources/converters.ts",
        "unit/spec/support/global.spec.ts"
    ],
    "exclude": []
}
